<mxStylesheet>
    <add as="custom">
        <add value="rectangle" as="shape"/>
        <add value="1" as="rounded"/>
        <add value="red" as="fillColor"/>
    </add>
    <add as="step">
        <add value="ext" as="shape"/>
        <add value="1" as="rounded"/>
        <add value="wrap" as="whiteSpace"/>
        <add value="#66FF66" as="fillColor"/>
    </add>
    <add as="event">
        <add value="message" as="shape"/>
        <add value="wrap" as="whiteSpace"/>
    </add>
    <add as="directionEvent">
        <add value="message" as="shape"/>
        <add value="wrap" as="whiteSpace"/>
    </add>
    <add as="startEvent">
        <add value="mxgraph.flowchart.on-page_reference" as="shape"/>
        <add value="wrap" as="whiteSpace"/>
        <add value="#ffffff" as="fillColor"/>
        <add value="#000000" as="strokeColor"/>
        <add value="2" as="strokeWidth"/>
    </add>
    <add as="intermediateEvent">
        <add value="doubleEllipse" as="shape"/>
        <add value="wrap" as="whiteSpace"/>
    </add>
    <add as="endEvent">
        <add value="mxgraph.flowchart.on-page_reference" as="shape"/>
        <add value="wrap" as="whiteSpace"/>
        <add value="#ffffff" as="fillColor"/>
        <add value="#000000" as="strokeColor"/>
        <add value="9" as="strokeWidth"/>
    </add>
    <add as="parallelGateway">
        <add value="orEllipse" as="shape"/>
        <add value="wrap" as="whiteSpace"/>
        <add value="ellipsePerimeter" as="perimeter"/>
        <add value="2" as="strokeWidth"/>
    </add>
    <add as="excludingGateway">
        <add value="sumEllipse" as="shape"/>
        <add value="wrap" as="whiteSpace"/>
        <add value="ellipsePerimeter" as="perimeter"/>
        <add value="2" as="strokeWidth"/>
    </add>
    <add as="rule">
        <add value="mxgraph.flowchart.document" as="shape"/>
        <add value="wrap" as="whiteSpace"/>
        <add value="#FFCCE6" as="fillColor"/>
        <add value="#000000" as="strokeColor"/>
        <add value="2" as="strokeWidth"/>
    </add>
    <add as="dataObject">
        <add value="note" as="shape"/>
        <add value="wrap" as="whiteSpace"/>
        <add value="16" as="size"/>
        <add value="2" as="strokeWidth"/>
    </add>
    <add as="solidLine">
        <add value="0" as="dashed"/>
    </add>
    <add as="dashedLine">
        <add value="1" as="dashed"/>
    </add>
</mxStylesheet>